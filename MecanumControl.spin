{  Project: EE-18 Assignment  Platform: Parallax Project USB Board  Revision: 1.2  Author: Teo Xin Yi  Date: 5th Mar 2022  Log:        Date: Desc        18/02/2022: Connect & Control Lite Kit using Mecanum wheels        5/3/2022:   Remove additional movements        16/3/2022: Integration}CON  ' RoboClaw 1 - Back  R1S1 = 3  R1S2 = 2  ' RoboClaw 2 - Front  R2S1 = 5  R2S2 = 4  ' Simple Serial  SSBaud = 57_600  ' Channel Stops  Ch1Stop = 64  Ch2Stop = 192  stepVal = 3VAR  long cogStack[64], cogIDNum, _Ms_001OBJ  MD[2]        : "FullDuplexSerial.spin"PUB Start(msVal, direction, mecspeed)                                           ' v1.1: change speed to mecspeed  _Ms_001 := msVal  StopCore  cogIDNum := cognew(mecCore(direction, mecspeed),@cogStack)                    ' v1.1: change speed to mecspeedPUB mecCore(direction, mecspeed)                                                ' v1.1 change speed to mecspeed  MecInit  repeat    case long[direction]      1:        Forward(long[mecspeed])      2:        Reverse(long[mecspeed])      3:        DiagNE(long[mecspeed])      4:        DiagSW(long[mecspeed])      5:        DiagNW(long[mecspeed])      6:        DiagSE(long[mecspeed])      7:        SideLeft(long[mecspeed])      8:        SideRight(long[mecspeed])      9:        StopPUB Forward(speed)  MD[0].Tx(Ch1Stop + speed)                                                       'Back right wheel  MD[0].Tx(Ch2Stop + speed)                                                       'Back left wheel  MD[1].Tx(Ch1Stop - speed)                                                       'Front right wheel  MD[1].Tx(Ch2Stop - speed)                                                       'Front left wheelPUB Reverse(speed) | i  MD[0].Tx(Ch1Stop - speed)                                                       'Back right wheel  MD[0].Tx(Ch2Stop - speed)                                                       'Back left wheel  MD[1].Tx(Ch1Stop + speed)                                                       'Front right wheel  MD[1].Tx(Ch2Stop + speed)                                                       'Front left wheelPUB SideLeft(speed) | i  MD[0].Tx(Ch1Stop - speed)                                                       'Back right wheel  MD[0].Tx(Ch2Stop + speed)                                                       'Back left wheel  MD[1].Tx(Ch1Stop + speed)                                                       'Front right wheel  MD[1].Tx(Ch2Stop - speed)                                                       'Front left wheelPUB SideRight(speed) | i  MD[0].Tx(Ch1Stop + speed)                                                       'Back right wheel  MD[0].Tx(Ch2Stop - speed)                                                       'Back left wheel  MD[1].Tx(Ch1Stop - speed)                                                       'Front right wheel  MD[1].Tx(Ch2Stop + speed)                                                       'Front left wheelPUB DiagNE(speed) | i  MD[0].Tx(Ch1Stop + speed)                                                       'Back right wheel  MD[0].Tx(Ch2Stop)                                                               'Back left wheel  MD[1].Tx(Ch1Stop - speed)                                                       'Front right wheel  MD[1].Tx(Ch2Stop)                                                               'Front left wheelPUB DiagNW(speed) | i  MD[0].Tx(Ch1Stop)                                                               'Back right wheel  MD[0].Tx(Ch2Stop + speed)                                                       'Back left wheel  MD[1].Tx(Ch1Stop)                                                               'Front right wheel  MD[1].Tx(Ch2Stop - speed)                                                       'Front left wheelPUB DiagSE(speed) | i  MD[0].Tx(Ch1Stop)                                                               'Back right wheel  MD[0].Tx(Ch2Stop - speed)                                                       'Back left wheel  MD[1].Tx(Ch1Stop)                                                               'Front right wheel  MD[1].Tx(Ch2Stop + speed)                                                       'Front left wheelPUB DiagSW(speed) | i  MD[0].Tx(Ch1Stop - speed)                                                       'Back right wheel  MD[0].Tx(Ch2Stop)                                                               'Back left wheel  MD[1].Tx(Ch1Stop + speed)                                                       'Front right wheel  MD[1].Tx(Ch2Stop)                                                               'Front left wheelPUB Stop | i  'Indivudual Channel Stop  MD[0].Tx(Ch1Stop)  MD[0].Tx(Ch2Stop)  MD[1].Tx(Ch1Stop)  MD[1].Tx(Ch2Stop)PUB StopAll  'Shut Down Transmission to Channel  MD[0].Tx(0)  MD[1].Tx(0)PRI MecInit  MD[0].Start(R1S2, R1S1, 0, SSBaud)  MD[1].Start(R2S2, R2S1, 0, SSBaud)  StopPRI StopCore                    'To stop the code in the core/cog and release the core/cog  if cogIDNum   cogstop(cogIDNum~)PRI Pause(ms) | t    'Pause Function  t := cnt - 1088  repeat(ms #> 0)    waitcnt(t += _Ms_001)  return